        "stopwords = stopwords.words(\"english\") + ['the', 'is', 'it', 'may']\n",
        "WNlemma = nltk.WordNetLemmatizer()\n",
        "snowball = nltk.SnowballStemmer('english')\n",
        "\n",
        "def pre_process(text):\n",
        "    tok = nltk.word_tokenize(text)\n",
        "    tok = [t for t in tok if t not in string.punctuation]\n",
        "    tok = [t.lower() for t in tok]\n",
        "    tok = [t for t in tok if not t.isnumeric()]\n",
        "    tok = [WNlemma.lemmatize(t) for t in tok]\n",
        "    tok = [t for t in tok if t not in stopwords]\n",
        "    final = \" \".join(tok)\n",
        "    return(final)"
      ]
    },
